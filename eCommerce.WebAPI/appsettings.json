    "accessTokenExpiration": 2000,
    "refreshTokenExpiration": 3000
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=eCommerce;Trusted_Connection=True;MultipleActiveResultSets=true;"
  }
}

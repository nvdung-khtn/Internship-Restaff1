{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfig": {
    "FrontEndUrl": "http://localhost:4200/"
  },
  "AllowedHosts": "*",
  "JwtTokenConfig": {
    "secret": "1234567890123456789",
    "issuer": "https://mywebapi.com",
